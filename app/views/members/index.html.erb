<% content_for(:title, "Members")%>
<% content_for(:menu,'members') %>
<div style="margin-left: 40px; margin-right: auto;padding-top: 20px;">
<% if @project.owner == current_user %>
  <%= render :partial => "inviteform", :locals => {:project => @project}%>
<% end %>
<%unless @project.nil?%>
  <% @project.members.each do |member| %>
    <div id="member<%=member.user_id%>">
    <%= render :partial => "member", :locals => {:member => member, :project => @project}%>
    </div >
  <% end %>
<% end %>
